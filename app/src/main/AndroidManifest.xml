<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.openxu.libdemo">

    <uses-permission android:name="android.permission.INTERNET" />
    <!-- 允许程序打开网络套接字 -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!-- 允许挂载和反挂载文件系统可移动存储 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <!--震动-->
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".evenbus.EventBusActivity1"/>
        <activity android:name=".view.ViewListActivity"/>
        <activity android:name=".view.HorizontalBarActivity"/>
        <activity android:name=".view.PieActivity"/>
        <activity android:name=".view.ProgressPieActivity"/>
        <activity android:name=".view.RoseActivity"/>
        <activity android:name=".view.VerticalBarActivity"/>
        <activity android:name=".view.XmStockChartActivity"/>
        <activity android:name=".view.XmStockChartActivity1"/>
        <activity android:name=".view.CoordinatorActivity"/>

    </application>

</manifest>